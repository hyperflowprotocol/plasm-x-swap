{
  "buildCommand": "cd plasm-x-swap && npm install && npm run build",
  "outputDirectory": "plasm-x-swap/dist",
  "installCommand": "cd plasm-x-swap && npm install",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
